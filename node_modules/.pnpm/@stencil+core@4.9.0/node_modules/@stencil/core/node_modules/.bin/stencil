#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sean/Documents/ionic/issues/stencil-pnpm/node_modules/.pnpm/@stencil+core@4.9.0/node_modules/@stencil/core/bin/node_modules:/Users/sean/Documents/ionic/issues/stencil-pnpm/node_modules/.pnpm/@stencil+core@4.9.0/node_modules/@stencil/core/node_modules:/Users/sean/Documents/ionic/issues/stencil-pnpm/node_modules/.pnpm/@stencil+core@4.9.0/node_modules/@stencil/node_modules:/Users/sean/Documents/ionic/issues/stencil-pnpm/node_modules/.pnpm/@stencil+core@4.9.0/node_modules:/Users/sean/Documents/ionic/issues/stencil-pnpm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sean/Documents/ionic/issues/stencil-pnpm/node_modules/.pnpm/@stencil+core@4.9.0/node_modules/@stencil/core/bin/node_modules:/Users/sean/Documents/ionic/issues/stencil-pnpm/node_modules/.pnpm/@stencil+core@4.9.0/node_modules/@stencil/core/node_modules:/Users/sean/Documents/ionic/issues/stencil-pnpm/node_modules/.pnpm/@stencil+core@4.9.0/node_modules/@stencil/node_modules:/Users/sean/Documents/ionic/issues/stencil-pnpm/node_modules/.pnpm/@stencil+core@4.9.0/node_modules:/Users/sean/Documents/ionic/issues/stencil-pnpm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/stencil" "$@"
else
  exec node  "$basedir/../../bin/stencil" "$@"
fi
